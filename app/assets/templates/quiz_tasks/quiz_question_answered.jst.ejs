<% if (correct) { %>
	<div class = "row">
		<div class = "eight columns centered mobile-four">
			<div class = "row panel">
				<div class = "tweleve columns mobile-four" style = "text-align: center; width: 100%">
					<h5>Good Job!</h5>
				</div>
				<div class = "tweleve columns mobile-four" style = "text-align: center; margin-top: 2em; width: 100%">
					<%= 'You got ' + points + ' points' %>
				</div>
				<div class = "tweleve columns mobile-four" style = "width: 100%; margin-top: 2em">
					<% if (last_q) { %>
						<button id = "done" type = "button" class = "success button" style = "width: 100%">Challenge Results</button>
					<% } else { %>
						<button id = "next" type = "button" class = "success button" style = "width: 100%">Next Question</button>
					<% } %>
				</div>
			</div>
		</div>
	</div>
<% } else { %>
	<div class = "row">
		<div class = "eight columns centered mobile-four">
			<div class = "row panel">
				<div class = "tweleve columns mobile-four" style = "text-align: center; width: 100%">
					<h5>You Fail...</h5>
				</div>
				<div class = "tweleve columns mobile-four" style = "text-align: center; margin-top: 2em; width: 100%">
					<%= 'The correct answer was: ' + quiz_q.get('correct') %>
				</div>
				<div class = "tweleve columns mobile-four" style = "width: 100%; margin-top: 2em">
					<% if (last_q) { %>
						<button id = "done" type = "button" class = "success button" style = "width: 100%">Challenge Results</button>
					<% } else { %>
						<button id = "next" type = "button" class = "alert button" style = "width: 100%">Next Question</button>
					<% } %>
				</div>
			</div>
		</div>
	</div>
<% } %>